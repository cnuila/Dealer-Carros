             console.error(\"Error updating document: \", error);\n                });\n            } else {\n                setSelected(estados[0])\n            }            \n        })\n    }\n\n    return (\n        <select className=\"px-3 bg-gray-200 ml-4 rounded-lg h-3/4\" name=\"estadoCarro\" value={selected} onChange={cambiarEstadoCarro}>\n            {props.estados.map((estado, index) => {\n                let valor = estado\n                if (index === 0) {\n                    return (<option key={index} value={valor} disabled hidden>\n                        {valor}\n                    </option>)\n                }\n                return (<option key={index} value={valor}>\n                    {valor}\n                </option>)\n            })}\n        </select>\n    )\n    /*else if (estado === \"Reparacion\") {\n        return (\n            <select className=\"px-3 bg-gray-200 ml-4 rounded-lg h-3/4\" id=\"estadoCarro\" onChange={cambiarEstadoCarro}>\n                <option value=\"Reparacion\">\n                    Reparacion\n                </option>\n                <option value=\"Disponible\">\n                    Disponible\n                </option>\n            </select>\n        )\n    } else if (estado === \"Repo\") {\n        return (\n            <select className=\"px-3 bg-gray-200 ml-4 rounded-lg h-3/4\" id=\"estadoCarro\" onChange={cambiarEstadoCarro}>\n                <option value=\"Repo\">\n                    Repo\n                </option>\n                <option value=\"Disponible\">\n                    Disponible\n                </option>\n                <option value=\"Reparacion\">\n                    Reparacion\n                </option>\n                <option value=\"Apartado\">\n                    Apartados\n                </option>\n            </select>\n        )\n    } else if (estado === \"Apartado\") {\n        return (\n            <select className=\"px-3 bg-gray-200 ml-4 rounded-lg h-3/4\" id=\"estadoCarro\" onChange={cambiarEstadoCarro}>\n                <option value=\"Apartado\">\n                    Apartados\n                </option>\n                <option value=\"Disponible\">\n                    Disponible\n                </option>\n            </select>\n        )\n    } else if (estado === \"Vendido\") {\n        return (\n            <select className=\"px-3 bg-gray-200 ml-4 rounded-lg h-3/4\" id=\"estadoCarro\" onChange={cambiarEstadoCarro}>\n                <option value=\"Vendido\">\n                    Vendido\n                    </option>\n                <option value=\"Disponible\">\n                    Disponible\n                    </option>\n                <option value=\"Reparacion\">\n                    Reparacion\n                    </option>\n                <option value=\"Repo\">\n                    Repo\n                    </option>\n            </select>\n        )\n    }*/\n}\n","/home/cnuila/Documents/Dealer-Carros/src/Components/Utilidades/Checkbox.js",[],"/home/cnuila/Documents/Dealer-Carros/src/Components/Inventario/Agregar Carro/ImagenPrincipal.js",[],"/home/cnuila/Documents/Dealer-Carros/src/Components/Utilidades/Counter.js",[],"/home/cnuila/Documents/Dealer-Carros/src/Components/LogIn.js",["121","122"],"import React, { useCallback, useContext} from 'react'\nimport logoImg from \"../Imágenes/img1.jpeg\";\nimport Mobile from \"../Imágenes/MobileLog.png\";\nimport {Redirect } from \"react-router\";\nimport fb from \"../firebase\"\nimport {AuthContext} from \"./Auth.js\"\n\nconst LogIn = ({ history }) => {\n\n    const handleLogin = useCallback(\n\n        async event => {\n            event.preventDefault();\n            const {email,password} = event.target.elements;\n            try{\n                await fb.auth().signInWithEmailAndPassword(email.value, password.value);\n                history.push(\"/\");\n            } catch (error){\n                alert(error);\n            }\n        },\n        [history]\n\n    );\n\n    const {currentUser} = useContext(AuthContext);\n\n    if(currentUser){\n        return <Redirect to=\"/\"/>;\n    }\n\n    return (\n        <div className=\"h-screen bg-gray-300\">\n            <div className=\"flex items-center justify-center h-full \">\n                <div className=\"flex max-w-sm md:max-w-3xl shadow-2xl border-0\">\n                    <div className=\"hidden md:block w-1/2 relative\">\n                        <img src={logoImg} className=\"w-full h-full  object-cover\" />\n                    </div>\n                    <div className=\"bg-white md:w-1/2 p-8\">\n\n                        <p className=\" sm:invisible md:visible lg:visible xl:visible  text-gray-900 text-3xl text-left font-bold\">LogIn</p>\n                        <p className=\" sm:invisible  md:invisible lg:invisible xl:invisible text-gray-900 text-3xl text-center font-bold\">Santos Motors</p>\n                        <p className=\" sm:invisible  md:invisible lg:invisible xl:invisible text-gray-900 text-xl text-center font-bold -p-8\">Group</p>\n \n                        <p className=\"text-gray-600 text-sm  my-4 font-sans text-center\">Bienvenido, inicie sesión por favor</p>\n                        <form onSubmit={handleLogin} className=\"bg-white shadow-md rounded px-8 pt-6 pb-8 mb-4 text-gray-600\">\n                            <div className=\"my-3\">\n                                <input className=\"shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:ring\" name=\"email\" type=\"email\" autoFocus  placeholder=\"Nombre de usuario\" />\n                                 {/*<input className=\"shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:ring\" id=\"username\" type=\"text\" autoFocus required value={email} onChange={(e) => setEmail(e.target.value)} placeholder=\"Nombre de usuario\" />*/}\n                                <p className=\"e