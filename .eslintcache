                {/*<div className=\"md:hidden absolute bg-gray-900 rounded-md w-2/3 h-80 \">\r\n                        <button className=\"col-span-2 top-0 right-0 p-1 ml-auto bg-transparent border-0 text-black float-right text-3xl leading-none font-semibold outline-none focus:outline-none\"\r\n                            onClick={() => handleEstadoModal(false)}>\r\n                            <span className=\"mb-4 mr-2 text-white h-8 w-8 text-3xl block outline-none focus:outline-none\">\r\n                                x\r\n                            </span>\r\n                        </button>\r\n                        <div className=\"h-64 w-64 object-cover bg-green-400\">\r\n                        </div>\r\n                            </div>*/}\r\n                </div>\r\n                <div className=\"opacity-50 fixed inset-0 z-10 bg-black\" ></div>\r\n            </>\r\n        );\r\n    }\r\n\r\n}\r\nexport default InfoCarro;\r\n\r\n\r\n/**/\r\n\r\n\r\n","C:\\Users\\Santos-Dell-Laptop\\Desktop\\Proyecto Santos\\Dealer-Carros\\src\\Components\\Inventario\\Agregar Carro\\InfoEstado.js",[],"C:\\Users\\Santos-Dell-Laptop\\Desktop\\Proyecto Santos\\Dealer-Carros\\src\\Components\\Utilidades\\Checkbox.js",[],"C:\\Users\\Santos-Dell-Laptop\\Desktop\\Proyecto Santos\\Dealer-Carros\\src\\Components\\Inventario\\ComboBoxCambiarEstado.js",[],"C:\\Users\\Santos-Dell-Laptop\\Desktop\\Proyecto Santos\\Dealer-Carros\\src\\Components\\Inventario\\Modificar.js",[],"C:\\Users\\Santos-Dell-Laptop\\Desktop\\Proyecto Santos\\Dealer-Carros\\src\\Components\\Inventario\\Agregar Carro\\OtrasImagenes.js",[],"C:\\Users\\Santos-Dell-Laptop\\Desktop\\Proyecto Santos\\Dealer-Carros\\src\\Components\\Ventas\\NuevaVenta.js",["147","148","149","150","151","152","153","154","155","156","157","158","159","160","161","162","163","164","165","166","167","168","169","170"],"import React, { useState } from 'react'\r\nimport moment from 'moment'\r\nimport InitialAgreement from './Agregar Venta/InitialAgreement'\r\nimport Docs from './Agregar Venta/Docs'\r\nimport Pasos from \"../Utilidades/Pasos\"\r\n\r\nexport default function NuevaVenta(props) {\r\n\r\n    const estadoInicial = {\r\n        pasos: [{ texto: \"Initial Agreement\", selected: true, terminado: false },\r\n        { texto: \"Docs y Observaciones\", selected: false, terminado: false }],\r\n        millaje: props.location.state.carro.millaje,\r\n        costumer: \"\",\r\n        address: \"\",\r\n        phoneNumber: \"\",\r\n        auto: props.location.state.carro.marca + \" \" + props.location.state.carro.modelo,\r\n        year: props.location.state.carro.ano,\r\n        vin: props.location.state.carro.id,\r\n        email: \"\",\r\n        precio: props.location.state.carro.precioFinal,\r\n        nuevoPrecio: props.location.state.carro.precioFinal,\r\n        down: props.location.state.carro.downPayment,\r\n        nuevoDown: props.location.state.carro.downPayment,\r\n        saldo: props.location.state.carro.precioFinal - props.location.state.carro.downPayment,\r\n        payments: 250.00,\r\n        fee: 75.00,\r\n        frecuencia: \"14 days\",\r\n        socialNumber: \"\",\r\n        taxes: props.location.state.carro.precioFinal * 0.06,\r\n        stickers: 180.00,\r\n        title: 120.00,\r\n        inspection: 120.00,\r\n        fee2: 100.00,\r\n        tagTotal: props.location.state.carro.precioFinal * 0.06 + 180.00 + 120.00 * 2 + 100.00,\r\n        endDate: \"MM/DD/YYYY\",\r\n        observaciones:\"\",\r\n    }\r\n\r\n    /*const estadoInicial = {\r\n        pasos: [{ texto: \"Initial Agreement\", selected: false, terminado: true },\r\n        { texto: \"Docs y Observaciones\", selected: true, terminado: false }],\r\n        millaje: props.location.state.carro.millaje,\r\n        costumer: \"DANIEL ENRIQUE HERNANDEZ HERNANDEZ\",\r\n        address: \"2014 POWHATAN RD HYATTSVILLE MD 20782\",\r\n        phoneNumber: \"(301) 404-8347\",\r\n        auto: props.location.st